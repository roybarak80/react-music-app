{
  "name": "react-music-app",
  "version": "1.0.0",
  "description": "How to use create-react-app with a custom Node API on Heroku",
  "engines": {
    "node": "8.9.x"
  },
  "scripts": {
    "start": "node server",
    "heroku-postbuild": "cd ui/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "ui/node_modules"
  ],
  "dependencies": {
    "cors": "^2.8.4",
    "express": "^4.14.1",
    "http": "0.0.0",
    "mongoose": "^5.0.6",
    "request": "^2.83.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/roybarak80/react-music-app.git"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {}
}
